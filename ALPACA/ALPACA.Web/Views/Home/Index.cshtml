@model ALPACA.Web.ViewModels.MainViewModel

 @{ Html.RenderPartial("_Email", Model.Email); }
<br />
<br />
<br />
<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8">
        <div role="tabpanel">
            <ul class="nav nav-tabs"  role="tablist">
                <li role="presentation" class="active"><a href="#listManagement" aria-controls="listManagement" role="tab" data-toggle="tab">List Management</a></li>
                <li role="presentation" ><a href="#userManagement" aria-controls="userManagement" role="tab" data-toggle="tab">User Management</a></li>
            </ul>
            <div class="tab-content">
                <div role="tabpanel" class="tab-pane active" id="listManagement"><br />@{ Html.RenderPartial("_Files"); }</div>
                <div role="tabpanel" class="tab-pane" id="userManagement"><br />@{if(ViewBag.IsAdmin)
                                                                                      {
                                                                                         Html.RenderPartial("_AdminUser", Model.Users);
                                                                                      }
                                                                                      else
                                                                                      {
                                                                                          Html.RenderPartial("_User");
                                                                                      }
                                                                                      }
                   </div>
                </div>
            </div>
        </div>
    </div>